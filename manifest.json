{
    "name": "LuckyNews Box",
    "version": "1.4.0",
    "description": "Configure News Sources by Yourself.",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "48": "images/icon48.png"
        },
        "default_popup": "index.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "content_scripts": [
        {
            "js": ["js/jquery-3.7.0.min.js"],
            "matches": ["<all_urls>"]
        }
    ],
	"permissions": [
		"<all_urls>"
	]
}
