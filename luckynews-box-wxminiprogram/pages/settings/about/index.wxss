.container {
  padding: 20px;
}

.settingscontent {
  margin-bottom: 20px;
}

.table {
  border-collapse: collapse;
  width: 100%;
  margin-top: 20px;
}

.row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #ccc;
  padding: 10px;
}

.header {
  font-weight: bold;
  background-color: lightgray;
}

.cell {
  flex: 1;
  text-align: center;
}