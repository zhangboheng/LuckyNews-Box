/* 设置页面顶部 logo 开始 */
.container-head {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 20px;
}
.container-item-text {
  text-align: center;
}

.inner-box {
  padding: 10px;
  width: 100%;
}

.inner-title {
  font-size: 24px;
  margin-left: 10px;
  font-weight: bold;
}

.container-head image {
  width: 96px;
  height: 96px;
}
/* 设置页面顶部 logo 结束 */

/* 设置页面底部功能区开始 */
.container-item {
  padding: 10px;
  border-bottom: 1px solid #eeeeee;
  display: flex;
  justify-content: space-between;
}

.container-item image {
  width: 24px;
  height: 24px;
}

.item-top {
  border-top: 1px solid #eeeeee;
}
/* 设置页面底部功能区结束 */