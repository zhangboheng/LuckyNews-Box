.container {
  text-align: center;
  margin: 0 auto;
}

.search-box {
  display: flex;
  align-items: center;
  background-color: #f5f5f5;
  border-radius: 20px;
  padding: 10px;
  margin: 20px;
}
.search-input {
  flex: 1;
  height: 20px;
  padding: 4px 8px;
  background-color: transparent;
  border: none;
  outline: none;
}
.search-button {
  margin-left: 10px;
  padding: 3px 6px;
  background-color: #1E90FF;
  border: none;
  border-radius: 20px;
  color: #fff;
  width: 24px;
  height: 24px;
}
.row {
  width: 90%;
  margin: 0 auto;
  text-align: center;
}
.rectangle {
  width: 100%;
  height: 100px;
  background-color: #ccc;
  margin-bottom: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
}